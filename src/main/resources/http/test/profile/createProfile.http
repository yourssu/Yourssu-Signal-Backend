Authorization: Bearer {{accessToken}}
Content-Type: application/json

{
  "gender": "FEMALE",
  "department": "학과",
  "birthYear": 2000,
  "animal": "DOG",
  "contact": "@leo",
  "mbti": "ISTJ",
  "nickname": "This nickname exceeds maximum length",
  "introSentences": ["나는 딸기를 좋아해", "나는 포도도 좋아해", "제일 좋아하는 음식은 치킨이야"]
